<Window x:Class="极简浏览器.Setting" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="设置" Height="200" Width="600"
        ResizeMode="NoResize">
    <Grid>
        <Label x:Name="label1" Content="主页：" HorizontalAlignment="Left" Margin="10,19,0,0"
                VerticalAlignment="Top" Height="34" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="78,23,0,0"
                TextWrapping="Wrap" VerticalAlignment="Top" Width="492" />
        <Button x:Name="button" Content="确定(_O)" HorizontalAlignment="Left" Margin="455,96,0,0"
                VerticalAlignment="Top" Width="115" Click="button_Click"
                Style="{StaticResource MaterialDesignPaperDarkButton}" />
        <Button x:Name="button1" Content="清除缓存" HorizontalAlignment="Left" Margin="235,58,0,0"
                VerticalAlignment="Top" Width="137" Click="button1_Click"
                Style="{StaticResource MaterialDesignPaperDarkButton}" />
        <Label x:Name="label2" Content="清除成功！" HorizontalAlignment="Left" Margin="245,157,0,0"
                VerticalAlignment="Top" Foreground="#FF3DF971" Visibility="Collapsed" />

    </Grid>
</Window>
