<Window x:Class="极简浏览器.Setting" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="设置" Height="150" Width="420"
        ResizeMode="NoResize">
    <Grid>
        <Label Content="主页：" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"
                Height="34" />
        <TextBox x:Name="MainPageBox" Height="35" Margin="80,11,10,0" TextWrapping="NoWrap"
                VerticalAlignment="Top" />
        <Button x:Name="OKButton" Content="确定" HorizontalAlignment="Right" Margin="0,0,10,10"
                VerticalAlignment="Bottom" Width="100" Click="OKButton_Click" />
        <Button x:Name="CacheButton" Content="清除缓存" HorizontalAlignment="Right" Margin="0,0,125,10"
                VerticalAlignment="Bottom" Width="100" Click="CacheButton_Click" />
        <Button x:Name="LogButton" Content="清除日志" HorizontalAlignment="Right" Margin="0,0,240,10"
                VerticalAlignment="Bottom" Width="100" Click="LogButton_Click" />
    </Grid>
</Window>
