    <Grid>
        <Label x:Name="label" Content="设置" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"
                FontSize="40" />
        <Label x:Name="label1" Content="主页：" HorizontalAlignment="Left" Margin="10,87,0,0" VerticalAlignment="Top"
                Height="34" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="78,91,0,0" TextWrapping="Wrap"
                VerticalAlignment="Top" Width="492" />
        <Button x:Name="button" Content="确定(_O)" HorizontalAlignment="Left" Margin="455,164,0,0" VerticalAlignment="Top"
                Width="115" Click="button_Click" />
        <Button x:Name="button1" Content="清除缓存" HorizontalAlignment="Left" Margin="235,126,0,0" VerticalAlignment="Top"
                Width="137" Click="button1_Click" />
        <Label x:Name="label2" Content="清除成功！" HorizontalAlignment="Left" Margin="245,157,0,0" VerticalAlignment="Top" Foreground="#FF3DF971" Visibility="Collapsed"/>

    </Grid>
