<Window x:Class="极简浏览器.Report" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="反馈错误"
        Width="750" Height="325" Loaded="Window_Loaded" ResizeMode="NoResize">
    <StackPanel>
        <Label x:Name="label" Margin="10,10,10,0" Content="抱歉，程序出现了错误" />
        <TextBox x:Name="textBox" Height="145" Margin="10,0,10,5" IsReadOnly="True"
                 TextWrapping="Wrap" />
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Label x:Name="label1" Margin="0,5" HorizontalAlignment="Left" VerticalAlignment="Top"
                   Content="请到" />
            <TextBox Width="617" Height="36" Margin="0,5" HorizontalAlignment="Left"
                     VerticalAlignment="Top" IsReadOnly="True" Text="https://github.com/KaiHuaDou/EasyBrowserAdvanced/issue" TextWrapping="Wrap" />
            <Label x:Name="label2" Margin="0,5" HorizontalAlignment="Left" VerticalAlignment="Top"
                   Content="上反馈" />
        </StackPanel>
        <Button x:Name="button1" Width="100" Margin="10,5" HorizontalAlignment="Right"
                Click="button1_Click" Content="确定" />

    </StackPanel>
</Window>
