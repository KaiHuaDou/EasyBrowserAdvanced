<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net452</TargetFramework>
        <OutputType>WinExe</OutputType>
        <IsWebBootstrapper>false</IsWebBootstrapper>
        <MapFileExtensions>true</MapFileExtensions>
        <TargetCulture>zh-Hans</TargetCulture>
        <ProductName>极简浏览器</ProductName>
        <PublisherName>KaiHuaDou</PublisherName>
        <SuiteName>极简浏览器</SuiteName>
        <ApplicationRevision>2</ApplicationRevision>
        <ApplicationVersion>3.4.7.2</ApplicationVersion>
        <UseApplicationTrust>false</UseApplicationTrust>
        <BootstrapperEnabled>false</BootstrapperEnabled>
        <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
        <UseWindowsForms>true</UseWindowsForms>
        <UseWPF>true</UseWPF>
        <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>
    <PropertyGroup>
        <ApplicationIcon>favicon\favicon.ico</ApplicationIcon>
    </PropertyGroup>
    <PropertyGroup>
        <StartupObject>极简浏览器.App.Program</StartupObject>
    </PropertyGroup>
    <PropertyGroup>
        <NoWin32Manifest>true</NoWin32Manifest>
        <PlatformTarget>x86</PlatformTarget>
    </PropertyGroup>
    <ItemGroup>
        <Resource Include="favicon\favicon.ico">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Resource>
        <Resource Include="favicon\favicon.old.ico" />
    </ItemGroup>
    <ItemGroup>
        <AppDesigner Include="Properties\" />
        <None Include="..\LICENSE">
          <Pack>True</Pack>
          <PackagePath>\</PackagePath>
        </None>
        <None Include="..\README.md">
          <Pack>True</Pack>
          <PackagePath>\</PackagePath>
        </None>
        <None Update="favicon\favicon.png">
          <Pack>True</Pack>
          <PackagePath>\</PackagePath>
        </None>
        <None Update="Resource\pepflashplayer.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Resource\rm.exe">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Resource\清理垃圾.bat">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Resource\清除足迹.bat">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Ruleset.ruleset">
            <SubType>Designer</SubType>
        </None>
        <None Update="极简浏览器.VisualElementsManifest.xml">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="cef.redist.x64">
            <Version>87.1.13</Version>
        </PackageReference>
        <PackageReference Include="cef.redist.x86">
            <Version>87.1.13</Version>
        </PackageReference>
        <PackageReference Include="CefSharp.Common">
            <Version>87.1.132</Version>
        </PackageReference>
        <PackageReference Include="CefSharp.Wpf">
            <Version>87.1.132</Version>
        </PackageReference>
    </ItemGroup>
    <PropertyGroup>
        <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
        <CodeAnalysisRuleSet>Ruleset.ruleset</CodeAnalysisRuleSet>
        <Title>极简浏览器</Title>
        <Authors>KaiHuaDou</Authors>
        <Company>KaiHuaDou Studio</Company>
        <Copyright>Copyright 2023 (c) KaiHuaDou Studio</Copyright>
        <PackageIcon>favicon.png</PackageIcon>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <AssemblyVersion>3.4.7.2</AssemblyVersion>
        <FileVersion>3.4.7.2</FileVersion>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
      <DebugType>full</DebugType>
      <WarningLevel>3</WarningLevel>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
      <DebugType>full</DebugType>
      <WarningLevel>3</WarningLevel>
    </PropertyGroup>
</Project>