<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net462</TargetFramework>
        <OutputType>WinExe</OutputType>
        <UseWindowsForms>true</UseWindowsForms>
        <UseWPF>true</UseWPF>
        <LangVersion>11.0</LangVersion>
        <StartupObject>极简浏览器.App.Program</StartupObject>
        <AnalysisLevel>latest-recommended</AnalysisLevel>
    </PropertyGroup>
    <PropertyGroup>
        <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
        <NoWin32Manifest>true</NoWin32Manifest>
        <PlatformTarget>x86</PlatformTarget>
        <DebugType>full</DebugType>
        <WarningLevel>7</WarningLevel>
    </PropertyGroup>
    <PropertyGroup>
        <Platforms>AnyCPU</Platforms>
        <ProductName>极简浏览器</ProductName>
        <PublisherName>KaiHuaDou</PublisherName>
        <SuiteName>极简浏览器</SuiteName>
        <ApplicationVersion>3.4.7.2</ApplicationVersion>
        <ApplicationIcon>Icon\favicon.ico</ApplicationIcon>
        <NeutralLanguage>zh-CN</NeutralLanguage>
    </PropertyGroup>
    <ItemGroup>
        <Folder Include="Properties" />
        <None Update="Icon\favicon.png">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <Resource Include="Icon\favicon.ico">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Resource>
        <None Update="Resources\pepflashplayer.dll">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Tools\rm.exe">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Tools\CleanData.bat">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="Tools\ClearBadRecord.bat">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
        <None Update="极简浏览器.VisualElementsManifest.xml">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </None>
    </ItemGroup>
    <ItemGroup>
        <None Update="Resources\Settings.settings">
            <Generator>SettingsSingleFileGenerator</Generator>
            <LastGenOutput>Settings.Designer.cs</LastGenOutput>
        </None>
        <Compile Update="Resources\GuiText.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>GuiText.resx</DependentUpon>
        </Compile>
        <Compile Update="Resources\Settings.Designer.cs">
            <DesignTimeSharedInput>True</DesignTimeSharedInput>
            <AutoGen>True</AutoGen>
            <DependentUpon>Settings.settings</DependentUpon>
        </Compile>
        <EmbeddedResource Update="Resources\GuiText.resx">
            <Generator>PublicResXFileCodeGenerator</Generator>
            <LastGenOutput>GuiText.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="cef.redist.x64">
            <Version>87.1.13</Version>
        </PackageReference>
        <PackageReference Include="cef.redist.x86">
            <Version>87.1.13</Version>
        </PackageReference>
        <PackageReference Include="CefSharp.Common">
            <Version>87.1.132</Version>
        </PackageReference>
        <PackageReference Include="CefSharp.Wpf">
            <Version>87.1.132</Version>
        </PackageReference>
    </ItemGroup>
</Project>